@model IEnumerable<Contact>
@{
    Layout = "_DashBoard";
}

<div class="untree_co-section before-footer-section">
    <div class="title">
        <i class="uil uil-tachometer-fast-alt"></i>
        <span class="text">View Contact</span>
    </div>
    <div class="container">
        <div class="row mb-5">

            <form class="col-md-12" method="post">
                <div class="site-blocks-table">
                    <table class="table">
                        <thead>
                            <tr>

                                <th class="product-Id">First Name</th>
                                <th class="product-name">Last Name</th>
                                <th class="product-price">Email</th>
                                <th class="product-quantity">Message</th>
                               
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                 
                                    <td>@item.Id</td>
                                    <td>@item.FName</td>
                                    <td>@item.LName</td>
                                    <td>@item.Email</td>
                                    <td>@item.Message</td>
                                    <td>


                                       @*  <a class="btn btn-success" asp-action="EditProduct" asp-route-id="@item.Id" role="button">
                                            <span class="material-symbols-outlined">
                                                Edit
                                            </span>
                                        </a>
                                        <a class="btn btn-danger " asp-action="Delete" asp-route-id="@item.Id" role="button">
                                            <span class="material-symbols-outlined">
                                                Delete
                                            </span>
                                        </a> *@
                                    </td>

                                </tr>
                            }

                        </tbody>
                    </table>
            </form>
        </div>




    </div>



